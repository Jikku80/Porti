extends base

block content
    h1.loader
        img.porti__off__img(src="/images/smalport.png", alt="porti__img")
        span vPor
        span.grn t
        span.nocaps i
    .login__head
        a.port_sec_head.hm(href="/")
            h1 Welcome, To 
                img.porti__off__img(src="/images/smalport.png" alt="porti__img")
                span vPor
                span.grn t
                span.nocaps i
        a.login(href="/account/login") LogIn | SignUp&#10162;
    .highlight__bod__sec
        .port__highlights__bod
            h2.cr__bod__headr Create a portfolio | You are not Logged In!
            p You need to log in to create a portfolio
            p Pick any of the layouts below and build your portfolio in just minutes! Fill the Form and Your All Set
            p Need tips Creating Your Portfolio Go To 
                a.portDet(href="/#pHelper") Portfolio Helper
            .layout__main
                a(href="/account/login").un_layout.nodeco
                    img(src="/images/layout1.png", loading="lazy", alt="layout1")
                    p PL Layout
                a(href="/account/login").un_layout.nodeco
                    img(src="/images/layout2.png", loading="lazy", alt="layout2")
                    p BG Layout
                a(href="/account/login").un_layout.nodeco
                    img(src="/images/layout3.png", loading="lazy", alt="layout3")
                    p ST Layout
                a(href="/account/login").un_layout.nodeco 
                    img(src="/images/layout4.png", loading="lazy", alt="layout1")
                    p BS Layout
                a(href="/account/login").un_layout.nodeco
                    img(src="/images/layout5.png", loading="lazy", alt="layout5")
                    p AT Layout (you can upload upto 20 images)
                a(href="/account/login").un_layout.nodeco
                    img(src="/images/layout6.png", loading="lazy", alt="layout6")
                    p NI Layout (no image required)
        .invi__highlights__section 
            h2.cr__bod__headr Create Invitation | You are not Logged In
